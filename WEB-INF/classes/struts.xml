<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>

    <constant name="struts.enable.DynamicMethodInvocation" value="false" />
    <constant name="struts.devMode" value="true" />

    <package name="default" namespace="/" extends="struts-default">

        <default-action-ref name="index" />

        <global-results>
            <result name="error">/error.jsp</result>
        </global-results>
    </package>
	<package name="HICHAT" extends="json-default">
		<action name="JWCHAT" class="com.wly.HeadPortrait.HeadPortrait" method="JWCHAT">	
			<result name="success">/JHB</result>
		</action>
		<action name="HeadPortrait" class="com.wly.HeadPortrait.HeadPortrait" method="imageToBase64">
			<result name="success" type="json">
				<param name="contentType">text/html</param>  
				<param name="json">jsonResult</param>
			</result>
		</action>
	</package>
</struts>
